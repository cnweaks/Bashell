<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.cnweak.rebash"
          android:versionCode="1"
          android:versionName="1.0.0"
          tools:ignore="GradleOverrides">
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"/>
    <uses-permission android:name="android.permissiocom.cnweak.rebash.ftpfileRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permissiocom.cnweak.rebash.ftpfileAKE_LOCK"/>


    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="23"
        tools:ignore="GradleOverrides"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup">

        <activity
            android:name=".CnweakActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EditActivity"
                  android:windowSoftInputMode="adjustResize"/>
        <activity android:name=".ViewActivity"
                  android:label="@string/goback"/>
        <activity android:name=".AboutActivity"
                  android:label="@string/goback"/>
        <activity android:name=".ShellActivity"
                  android:label="@string/goback"/>
        <activity android:name=".SettingsActivity"
                  android:label="@string/goback"/>
        <activity android:name=".FileActivity"
                  android:label="@string/goback"/>
        <activity android:name=".html.GetCodeforgeCode"
                  android:label="@string/goback"/>
        <activity android:name=".HtmlViewActivity"
                  android:label="@string/goback"/>
    </application>

</manifest>
